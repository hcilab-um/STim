<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
			<section name="STimWPF.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
		</sectionGroup>
		<sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
			<section name="STimWPF.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
		</sectionGroup>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>
	<applicationSettings>
		<STimWPF.Properties.Settings>
			<setting name="DestFolder" serializeAs="String">
				<value>C:\Users\{USERNAME}\Desktop\</value>
			</setting>
			<setting name="SkeletonBufferSize" serializeAs="String">
				<value>10</value>
			</setting>
			<setting name="PlayerBufferSize" serializeAs="String">
				<value>10</value>
			</setting>
		</STimWPF.Properties.Settings>
	</applicationSettings>
	<userSettings>
		<STimWPF.Properties.Settings>
			<setting name="DecimalPrecision" serializeAs="String">
				<value>5</value>
			</setting>
		</STimWPF.Properties.Settings>
	</userSettings>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
	</startup>
	<log4net>
		<appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="C:\Users\{USERNAME}\Desktop\Logs\realTimeCaptureLog\capture-log.txt" />
			<appendToFile value="true" />
			<rollingStyle value="Size" />
			<maxSizeRollBackups value="10" />
			<maximumFileSize value="100KB" />
			<staticLogFileName value="true" />
			<layout type="log4net.Layout.PatternLayout">
				<header value="DateTime;Anchoring;TypingMethod;SelectionMethod;ArmStretch;Dexterity;PlaneSize;TotalTime;ErrorRateLanding;ErrorRateSelecting;CoM.L;CoM.Theta;Elbow.Theta;Wrist.Theta;HumanTorqueSum;AvgHumanTorque;MaxAvgHumanTorquePercent;AvgTbEndurance;TbEnduranceLostPercent;TbEnduranceLostRate;AvgCoMHumanForce;CoMHumanForceSum;MaxAvgCoMHumanForcePercent;AvgFbEndurance;FbEnduranceLostPercent;FbEnduranceLostRate;MeasuredTorqueSum;GravityTorqueSum;InertialTorqueSum;AvgMeasuredTorque;AvgGravityTorque;AvgInertialTorque;CoM.L;CoM.Theta;Elbow.Theta;Wrist.Theta;HumanTorqueSum;AvgHumanTorque;MaxAvgHumanTorquePercent;AvgTbEndurance;TbEnduranceLostPercent;TbEnduranceLostRate;AvgCoMHumanForce;CoMHumanForceSum;MaxAvgCoMHumanForcePercent;AvgFbEndurance;FbEnduranceLostPercent;FbEnduranceLostRate;MeasuredTorqueSum;GravityTorqueSum;InertialTorqueSum;AvgMeasuredTorque;AvgGravityTorque;AvgInertialTorque;Gender;EnteredSentence"/>
				<conversionPattern value="%newline%message" />
			</layout>
		</appender>
		<root>
			<level value="ALL" />
			<appender-ref ref="RollingFileAppender" />
		</root>
	</log4net>
</configuration>